{
  "function": {
    "todohandler": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "api": {
    "todoAPIs": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "todohandler",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "storage": {
    "dynamotodo": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  }
}